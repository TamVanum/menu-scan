<% if(menu) { %>
    <!doctype html>
    <html lang="en">

    <head>
        <title>Carousel 03</title>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

        <link href="https://fonts.googleapis.com/css?family=Playfair+Display:400,400i,700,700i&display=swap"
            rel="stylesheet">

        <!-- Favicon-->
        <link rel="icon" type="image/x-icon" href="assets/favicon.ico" />
        <!-- Font Awesome icons (free version)-->
        <script src="https://use.fontawesome.com/releases/v6.3.0/js/all.js" crossorigin="anonymous"></script>
        <!-- Google fonts-->
        <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet" type="text/css" />
        <link href="https://fonts.googleapis.com/css?family=Roboto+Slab:400,100,300,700" rel="stylesheet"
            type="text/css" />
        <!-- Core theme CSS (includes Bootstrap)-->
        <link href="/estilo_tienda.css" rel="stylesheet" />
    </head>

    <body id="page-top">

        <nav class="navbar navbar-expand-lg navbar-dark fixed-top" id="mainNav">
            <div class="container">
                <a class="" href="#page-top"><img src="/img/banners/<%= menu.logo_menu %>" width="100%" height="100px"
                        alt="..." /></a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                    data-bs-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false"
                    aria-label="Toggle navigation">
                    Menu
                    <i class="fas fa-bars ms-1"></i>
                </button>
                <div class="collapse navbar-collapse" id="navbarResponsive">
                    <ul class="navbar-nav text-uppercase ms-auto py-4 py-lg-0">
                        <li class="nav-item px-3"><a class="nav-link" href="#portfolio">Opciones</a></li>
                        <li class="nav-item px-3"><a class="nav-link" href="/carousel">Visualizacion</a></li>
                    </ul>
                </div>
            </div>
        </nav>
        <!-- Masthead-->
        <header class="masthead" style="background-image: url('/img/banners/<%= menu.banner_menu %>');">
            <div class="container">
                <div class="masthead-subheading">Bienvenido a!</div>
                <div class="masthead-heading text-uppercase">
                    <%= menu.nombre_menu %>
                </div>
                <a class="btn btn-danger btn-xl text-uppercase text-black" href="#portfolio">Menu</a>
            </div>
        </header>
        <!-- Portfolio Grid-->
        <section class="page-section bg-light" id="portfolio">
            <div class="container">
                <div class="text-center">
                    <h2 class="section-heading text-uppercase">Opciones</h2>
                    <h3 class="section-subheading text-muted">Tus opciones como administrador.</h3>
                </div>
                <div class="row" id="alimentos-container">

                    <div class="col-lg-6 col-sm-12 mb-6">
                        <!-- Portfolio item 1 -->
                        <div class="portfolio-item">
                            <a class="portfolio-link" data-bs-toggle="modal">
                                <img class="" src="/img/alimentos.jpg" height="300px" width="100%" alt="..." />
                            </a>
                            <div class="portfolio-caption">
                                <div class="portfolio-caption-heading">Gestionar Alimentos</div>
                                <div class="portfolio-caption-subheading text-muted fs-4"></div>
                                <a class="btn btn-primary btn-xl text-uppercase text-black" href="/admin_a">Entrar</a>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-6 col-sm-12 mb-6">
                        <!-- Portfolio item 1 -->
                        <div class="portfolio-item">
                            <a class="portfolio-link" data-bs-toggle="modal">
                                <img class="" src="/img/categorias.jpg" height="300px" width="100%" alt="..." />
                            </a>
                            <div class="portfolio-caption">
                                <div class="portfolio-caption-heading">Gestionar Categorias</div>
                                <div class="portfolio-caption-subheading text-muted fs-4"></div>
                                <a class="btn btn-primary btn-xl text-uppercase text-black" href="/admin_c">Entrar</a>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </section>



        <div class="mx-5">
            <a class="btn btn-primary btn-xl m-3 text-black" href="/crear_alimento">Crear Nuevo Alimento <i
                    class="fa-solid fa-plus fa-lg"></i></i></a>
            <div class="container-fluid table-responsive">
                <table id="alimentos" class="table table-striped text-center " style="width:100%">
                    <thead>
                        <tr class="table-warning ">
                            <th>Nombre</th>
                            <th>Categoria</th>
                            <th>Precio</th>
                            <th>Foto</th>
                            <th>Estado</th>
                            <th>Acciones</th>
                        </tr>
                    </thead>
                    <tbody class="text-center">
                        <% alimentos.forEach((alimento)=> { %>
                            <tr>
                                <td>
                                    <%= alimento.nombre_alimento %>
                                </td>
                                <td>
                                    <%= alimento.nombre_categoria %>
                                </td>
                                <td>
                                    <%= alimento.precio %>
                                </td>
                                <td>
                                    <img class="rounded-circle" src="/img/alimentos/<%= alimento.foto_alimento %>"
                                        style="width: 80px; height: 80px;" alt="">
                                </td>
                                <td>
                                    <%=alimento.nombre %>
                                </td>
                                <td>
                                    <a class="btn btn-info" href="/editar_alimento/<%= alimento.alimento_id %>"><i
                                            class="fa-solid fa-pen-to-square"></i></a>

                                    <% if(alimento.estado_alimento_id==1){ %>
                                        <a class="btn btn-danger"
                                            href="/eliminar_alimento/<%= alimento.alimento_id %>"><i
                                                class="fa-solid fa-trash"></i></a>
                                        <% } else{ %>
                                            <a class="btn btn-success"
                                                href="/habilitar_alimento/<%= alimento.alimento_id %>"><i
                                                    class="fa-solid fa-square-plus"></i></a>
                                            <% } %>
                                </td>


                            </tr>
                            <% }); %>
                    </tbody>
                </table>
            </div>
        </div>



